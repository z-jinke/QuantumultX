#我的页面
^https:\/\/api\.coolapk\.com\/v6\/account\/loadConfig url script-response-body https://raw.githubusercontent.com/z-jinke/QuantumultX/main/scripts/Coolapk.js
#首页与搜索
^https:\/\/api\.coolapk\.com\/v6\/main\/(indexV8|search) url script-response-body https://raw.githubusercontent.com/z-jinke/QuantumultX/main/scripts/Coolapk.js
#初始化配置
^https:\/\/api\.coolapk\.com\/v6\/main\/init url script-response-body https://raw.githubusercontent.com/z-jinke/QuantumultX/main/scripts/Coolapk.js
